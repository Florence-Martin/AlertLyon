{
  "expo": {
    "name": "LyonAlert",
    "slug": "LyonAlert",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-router"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.flomar.lyonalert"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.flomar.LyonAlert"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "flomar",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/56c96177-9f2e-4dbe-bf26-1b67b8369162"
    },
    "extra": {
      "eas": {
        "projectId": "56c96177-9f2e-4dbe-bf26-1b67b8369162"
      }
    }
  }
}
